{
    "dbt.deferConfigPerProject": {
        "aw_project": {
            "favorState": false
        }
    },

      "files.associations": {
    "**/aw_project/**/*.sql": "jinja-sql",
    "**/target/**": "",
    "**/<aw_project>/**/*.yaml": "jinja-yaml",
    "**/<aw_project>/**/*.yml": "jinja-yaml",
    "**/<aw_project>/**/docs/**/*.md": "jinja-md",
    "*.dat": "csv (pipe)",
    "*.csv": "csv (semicolon)"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "dbt.perspectiveTheme": "Vintage"

}
